{
    "presets": [
        "@babel/env",
        "@babel/typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "@babel/plugin-transform-react-jsx",
            {
                "runtime": "classic",
                "pragma": "tjsx.createElement"
            }
        ]
    ]
}